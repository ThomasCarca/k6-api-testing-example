{
  "scenarios": {
    "example_scenario_1": {
      "executor": "shared-iterations",
      "startTime": "5s",
      "gracefulStop": "5s",
      "env": {
        "EXAMPLEVAR": "testing"
      },
      "tags": {
        "example_tag": "testing"
      },
      "vus": 10,
      "iterations": 200,
      "maxDuration": "10s",
      "exec": "slow"
    },
    "example_scenario_2": {
      "executor": "per-vu-iterations",
      "vus": "5",
      "iterations": "30",
      "exec": "slow"
    },
    "example_scenario_3": {
      "executor": "constant-arrival-rate",
      "rate": 200,
      "duration": "10s",
      "preAllocatedVUs": 50,
      "maxVUs": 100,
      "exec": "fast"
    }
  },
  "thresholds": {
    "http_req_duration{type:fast_endpoint}": [
      {
        "threshold": "p(95) < 80",
        "abortOnFail": true
      }
    ],
    "http_req_duration{type:slow_endpoint}": [
      {
        "threshold": "p(90) < 265",
        "abortOnFail": false
      }
    ]
  }
}
